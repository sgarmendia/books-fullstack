{
	"AWSEBDockerrunVersion": 3,
	"containerDefinitions": [
		{
			"name": "nginx",
			"image": "sgarmendia/books-nginx",
			"memory": 128,
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["client", "server"]
		},
		{
			"name": "client",
			"image": "sgarmendia/books-client",
			"memory": 128,
			"essential": true
		},
		{
			"name": "server",
			"image": "sgarmendia/books-server",
			"memory": 128,
			"essential": true
		}
	]
}
